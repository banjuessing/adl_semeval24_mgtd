{
  "fixed_params": {
    "model_name": "roberta-base",
    "text_col": "text",
    "label_col": "label",
    "train_data_path": "path/to/train_set_comp.jsonl",
    "val_data_path": "path/to/val_set_comp.jsonl",
    "regex_query": "\\\\s+",
    "max_length": 512,
    "output_dir": "path/to/model_output",
    "logging_dir": "path/to/model_output_logs",
    "gradient_accumulation_steps": 2, 
    "fp16": true,
    "dataloader_num_workers": 4,
    "evaluation_strategy": "epoch",
    "logging_strategy": "steps",
    "save_strategy": "epoch",
    "seed": 42,
    "data_seed": 42,
    "eval_metric": "eval_accuracy", 
    "n_trials": 20,
    "n_startup_trials": 5,
    "interval_steps": 2,
    "n_warmup_steps": 2,
    "study_results": "path/to/save/study_results.json",
    "per_device_eval_batch_size": 40
  },
    "tunable_params": {
      "learning_rate": {
        "type": "float",
        "args": [1e-3, 5e-4]
    },
    "num_train_epochs": {
      "type": "int",
      "args": [2, 10]
    },
    "per_device_train_batch_size": {
      "type": "categorical",
      "args": [4, 8, 12]
    },
    "warmup_steps": {
      "type": "int",
      "args": [0, 500]
    },
    "weight_decay": {
      "type": "float",
      "args": [0.001, 0.003]
    }
  }
}

